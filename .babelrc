{
  "env": {
    "production": {
      "presets": [
        "stage-0", [
          "env", {
              "debug": true,
              "modules": false,
              "useBuiltIns": true,
              "targets": {
                "browsers": "defaults",
                "node": "current"
              }
          }
        ]
      ],
      "plugins": ["transform-decorators-legacy", "transform-class-properties"]
    },
    "development": {
      "presets": [
        "stage-0", [
          "env", {
            "debug": false,
            "modules": false,
            "useBuiltIns": true,
            "targets": {
              "browsers": ["chrome >= 56"],
              "node": "current"
            }
          }
        ]
      ],
      "plugins": ["transform-decorators-legacy", "transform-class-properties"]
    },
    "cli": {
      "presets": [
        "stage-0", [
          "env", {
            "debug": true,
            "modules": false,
            "targets": {
              "node": "current"
            }
          }
        ]
      ],
      "plugins": ["transform-decorators-legacy", "transform-class-properties"]
    }
  }
}
